{
  "name": "react-app-components",
  "version": "1.0.3",
  "description": "react-app-components",
  "author": "Hemerson Carlin <mersocarlin@gmail.com>",
  "scripts": {
    "build-storybook": "build-storybook",
    "clean": "rimraf lib lib-es6",
    "coverage": "jest --config .jestrc --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "./node_modules/.bin/eslint src/",
    "prepublish": "yarn run transpile",
    "pretranspile": "yarn run clean",
    "start": "start-storybook -p 6006",
    "test": "jest --config .jestrc",
    "transpile": "yarn transpile:es5 && yarn transpile:es6",
    "transpile:es5": "cross-env NODE_ENV=production BABEL_ENV=es5 babel src --out-dir lib --copy-files",
    "transpile:es6": "cross-env NODE_ENV=production BABEL_ENV=es6 babel src --out-dir lib-es6 --copy-files"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mersocarlin/react-app-components"
  },
  "keywords": [],
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-datepicker": "^0.41.1",
    "react-dom": "^15.4.2",
    "react-select": "^1.0.0-rc.3",
    "recompose": "^0.22.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "coveralls": "^2.12.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.1",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2"
  },
  "license": "MIT"
}
